                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT
  CAST(m.ID as varchar(200)) AS operating_group_uid
, m.name AS operating_group_desc
, m.Code AS operating_group_code
, 'BU' AS operating_group_type_uid
, ISNULL(m.UpdateDateTime, m.InsertDateTime) as source_revision_dtm
, u.NetworkID AS source_revision_actor
FROM
LUV_Master m
INNER JOIN LUV_MasterType t ON t.ID = m.TypeID
LEFT JOIN EmployeeMaster u ON u.ID = m.InsertByID
WHERE
t.Name = 'Business Unit'
AND m.ActiveIndicator = 1
AND m.Code NOT IN ('NAT','SSA')


UNION ALL

SELECT
  CAST(m.ID as varchar(200)) AS operating_group_uid
, m.name AS operating_group_desc
, m.Code AS operating_group_code
, 'OPS' AS operating_group_type_uid
, ISNULL(m.UpdateDateTime, m.InsertDateTime) as source_revision_dtm
, u.NetworkID AS source_revision_actor
FROM
LUV_Master m
INNER JOIN LUV_MasterType t ON t.ID = m.TypeID
LEFT JOIN EmployeeMaster u ON u.ID = m.InsertByID
WHERE
t.Name = 'Business Unit'
AND m.ActiveIndicator = 1
AND m.Code IN ('NAT','SSA')


UNION ALL

SELECT
CAST(m.ID as varchar(200)) AS operating_group_uid
, m.name AS operating_group_desc
, m.NameAbbreviation AS operating_group_code
, 'CAP' AS operating_group_type_uid
, ISNULL(m.UpdateDateTime, m.InsertDateTime) as source_revision_dtm
, u.NetworkID AS source_revision_actor
FROM
LUV_Master m
INNER JOIN LUV_MasterType t ON t.ID = m.TypeID
LEFT JOIN EmployeeMaster u ON u.ID = m.InsertByID
WHERE
t.Name IN ('Service Line','Industry Vertical')
AND m.ParentID = m.ID
AND m.ActiveIndicator = 1

ORDER BY operating_group_uid</property>
